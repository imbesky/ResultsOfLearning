public class GuGuDan {

	public static void main(String[] args) {
		int num;
		int time;
		
		for(num=1; num<=9; num++) {
			System.out.println(num+"ë‹¨");
			for(time=1; time <=9;time++) {
				int result = num*time;
				System.out.println(num+"X"+time+"="+result);
			}
			System.out.println();
		}

	}

}
